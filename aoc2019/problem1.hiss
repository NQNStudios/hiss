---
(setq modules (map (splitLines (getContent "aoc2019/input1-1.txt")) parseInt))
---
(defun fuel (module)
    (setlocal number (haxe- (floor (haxe/ module 3)) 2))
    (if (haxe< number 0) 
        0
        (+ number (fuel number))))
---
(print (eval (cons '+ (map modules fuel))))